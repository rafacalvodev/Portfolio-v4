{
  "name": "eleventy-i18n",
  "version": "1.0.0",
  "description": "Minimal starter for localized content, using Eleventy's own Internationalization (I18n) plugin",
  "license": "ISC",
  "author": {
    "name": "Lene Saile",
    "url": "https://www.lenesaile.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-i18n.git"
  },
  "engines": {
    "node": ">=16.x.x"
  },
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/assets/sass:dist/assets/css",
    "build:sass": "sass --no-source-map src/assets/sass:dist/assets/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "html-minifier-terser": "^7.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.61.0"
  }
}
